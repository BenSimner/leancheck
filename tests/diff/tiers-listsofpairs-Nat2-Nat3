tiers :: [Nat2->Nat3]  =
  [ [ \x -> case x of
              0 -> 0
              1 -> 0
    ]
  , [ \x -> case x of
              0 -> 1
              1 -> 1
    ]
  , [ \x -> case x of
              0 -> 1
              1 -> 0
    , \x -> case x of
              0 -> 0
              1 -> 1
    , \x -> case x of
              0 -> 2
              1 -> 2
    ]
  , [ \x -> case x of
              0 -> 2
              1 -> 0
    , \x -> case x of
              0 -> 0
              1 -> 1
    , \x -> case x of
              0 -> 1
              1 -> 0
    , \x -> case x of
              0 -> 0
              1 -> 2
    ]
  , [ \x -> case x of
              0 -> 0
              1 -> 2
    , \x -> case x of
              0 -> 2
              1 -> 1
    , \x -> case x of
              0 -> 1
              1 -> 2
    , \x -> case x of
              0 -> 2
              1 -> 0
    ]
  , [ \x -> case x of
              0 -> 1
              1 -> 2
    , \x -> case x of
              0 -> 2
              1 -> 1
    ]
  ]

map length (tiers :: [[ Nat2->Nat3 ]])  =  [1,1,3,4,4,2]

length (list :: [ Nat2->Nat3 ])  =  Infinity
