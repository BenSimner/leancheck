tiers :: [Nat2->Nat2]  =
  [ [ \x -> case x of
              0 -> 0
              1 -> 0
    ]
  , [ \x -> case x of
              0 -> 1
              1 -> 1
    ]
  , [ \x -> case x of
              0 -> 1
              1 -> 0
    , \x -> case x of
              0 -> 0
              1 -> 1
    ]
  , [ \x -> case x of
              0 -> 0
              1 -> 1
    , \x -> case x of
              0 -> 1
              1 -> 0
    ]
  ]

map length (tiers :: [Nat2->Nat2])  =  [1,1,2,2]
